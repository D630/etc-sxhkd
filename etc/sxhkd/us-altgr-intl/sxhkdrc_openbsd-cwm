super + slash
    exec xorg-raise-xwin

super + alt + s
    exec run-scrot -m

super + shift + s
    exec run-scrot

super + Return
    exec x-terminal

super + shift + Return
    exec xterm

super + shift + Delete
    exec xorg-lock

super + p
    exec dacti

super + r
    exec arandr

super + y
    exec stj1

super + minus
    exec xorg-switch-to-desktop

XF86Audio{Raise,Lower}Volume
    exec volume {+,-}

XF86AudioMute
    exec volume toggle

super + XF86AudioMute
    exec mpc -q toggle

super + XF86Audio{Lower,Raise}Volume
    exec mpc -q {prev,next}
